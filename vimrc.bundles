if &compatible
  set nocompatible
end

filetype off

" set the runtime path to include Vundle and initialize
set rtp+=~/.vim/bundle/Vundle.vim
call vundle#rc()

Plugin 'gmarik/Vundle.vim'

" vkchmck/im-coffee-script
" CoffeeScript support for vim
" https://github.com/kchmck/vim-coffee-script
Plugin 'kchmck/vim-coffee-script'

" tpope/vim-rails'
" rails.vim: Ruby on Rails power tools
" https://github.com/tpope/vim-rails
Plugin 'tpope/vim-rails'

" vim-ruby/vim-ruby
" Vim/Ruby Configuration Files
" https://github.com/vim-ruby/vim-ruby
Plugin 'vim-ruby/vim-ruby'

" Plugin 'vim-scripts/ctags.vim'
" Plugin 'sickill/vim-sunburst'

" elzr/vim-json
" A better JSON for Vim
" https://github.com/elzr/vim-json
Plugin 'elzr/vim-json'

" tpope/vim-dispatch
" dispatch.vim: asynchronous build and test dispatcher
" https://github.com/tpope/vim-dispatch
Plugin 'tpope/vim-dispatch'

" airblade/vim-gitgutter
" https://github.com/airblade/vim-gitgutter
" A Vim plugin which shows a git diff in the 'gutter' (sign column). It shows
" whether each line has been added, modified, and where lines have been removed.
" You can also stage and revert individual hunks.
" Plugin 'airblade/vim-gitgutter' " shows a git diff in the 'gutter' (sign column)

" kshenoy/vim-signature
" https://github.com/kshenoy/vim-signature
" Plugin to toggle, display and navigate marks
" Plugin 'kshenoy/vim-signature'

" kien/ctrlp.vim
" https://github.com/kien/ctrlp.vim
" Fuzzy file, buffer, mru, tag, etc finder.
Plugin 'kien/ctrlp.vim'

" altercation/vim-colors-solarized
" precision colorscheme for the vim text editor
" https://github.com/altercation/vim-colors-solarized
Plugin 'altercation/vim-colors-solarized'

" ngmy/vim-rubocop
" The Vim RuboCop plugin runs RuboCop and displays the results in Vim
" https://github.com/ngmy/vim-rubocop
Plugin 'ngmy/vim-rubocop'

" tpope/vim-surround
" https://github.com/tpope/vim-surround
" surround.vim: quoting/parenthesizing made simple
Plugin 'tpope/vim-surround'

" valloric/MatchTagAlways
" https://github.com/valloric/MatchTagAlways
" A Vim plugin that always highlights the enclosing html/xml tags
" Plugin 'valloric/MatchTagAlways'

" cakebaker/scss-syntax.vim
" https://github.com/cakebaker/scss-syntax.vim
" Vim syntax file for scss (Sassy CSS)
