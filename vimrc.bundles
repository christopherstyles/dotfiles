" wincent/terminus
" Enhanced terminal integration for Vim
" https://github.com/wincent/terminus
Plugin 'wincent/terminus'

" ferret
" https://github.com/wincent/ferret
" Enhanced multi-file search for Vim

" emmet-vim
" Emmet for vim: http://emmet.io/ http://mattn.github.io/emmet-vim
Plugin 'mattn/emmet-vim'

" tpope/vim-unimpaired
" https://github.com/tpope/vim-unimpaired
" unimpaired.vim: pairs of handy bracket mappings
Plugin 'tpope/vim-unimpaired'

" wellle/targets.vim
" Vim plugin that provides additional text objects.
" https://github.com/wellle/targets.vim
Plugin 'wellle/targets.vim'

" slim-template/vim-slim
" Syntax highlighting for VIM
" https://github.com/slim-template/vim-slim
Plugin 'slim-template/vim-slim'

" Thoughtbot

" thoughtbot/vim-rspec'
" Run Rspec specs from Vim
" https://github.com/thoughtbot/vim-rspec
" Plugin 'thoughtbot/vim-rspec'

" gilsondev/searchtasks.vim
" Plugin to search the labels often used as TODO, FIXME and XXX.
" https://github.com/gilsondev/searchtasks.vim
" Plugin 'gilsondev/searchtasks.vim'

" tpope/vim-eunuch
" eunuch.vim: helpers for UNIX
" https://github.com/tpope/vim-eunuch
Plugin 'tpope/vim-eunuch'

